package com.hongedu.honghr.honghr.common.enums;

public enum EnumAbsenceApplyType {
	LEAVE("101105","调休"), ANNUAL_LEAVE("101101","年假"),SICK_LEAVE("101107","病假"),CASUAL_LEAVE("101108","事假"),PUBLIC_LEAVE("101109","公假"),MARRIAGE_LEAVE("101102","婚假"),
	BEREAVEMENT_LEAVE("101103","丧假"), MATERNITY_LEAVE("101104","产检假"), PATERNITY_LEAVE("101106","陪产假");
	
	private String code;
	
	private String value;
	
	private EnumAbsenceApplyType(String code, String value) {
		this.code = code;
		this.value = value;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}
}